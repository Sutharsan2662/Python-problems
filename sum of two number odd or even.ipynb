   "source": [
    "# Given 2 numbers N and M add both the numbers and check whether the sum is odd or even.\n",
    "# Sample Testcase :\n",
    "# INPUT\n",
    "# 9 2\n",
    "# OUTPUT\n",
    "# odd\n",
    "a,b = map(int,input().split())\n",
    "\n",
    "if (a+b)%2 == 0:\n",
    "    print(\"even\")\n",
    "else:\n",
    "    print(\"odd\")"
   ]
  }
