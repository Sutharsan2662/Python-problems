   "source": [
    "#Removing white spaces in a sentence and printing the number of letters\n",
    "userInput = input()\n",
    "\n",
    "arr = list(userInput)\n",
    "\n",
    "for i in arr[:]:\n",
    "    if i == \" \":\n",
    "        arr.remove(i)\n",
    "\n",
    "    \n",
    "print(len(arr))"
   ]
  }
