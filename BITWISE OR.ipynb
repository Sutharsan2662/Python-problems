   "source": [
    "# Given a number N and an array of N elements, find the Bitwise OR of the array elements.\n",
    "# Input Size : N <= 100000\n",
    "# Sample Testcase :\n",
    "# INPUT\n",
    "# 2\n",
    "# 2 4\n",
    "# OUTPUT\n",
    "# 6\n",
    "\n",
    "n = int(input())\n",
    "\n",
    "a = list(map(int,input().split()))\n",
    "\n",
    "res = 0\n",
    "for i in a:\n",
    "    res |= i\n",
    "    \n",
    "print(res)\n",
    "    \n",
    "    "
