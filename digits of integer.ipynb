   "source": [
    "#printing digits of integer\n",
    "userInput = int(input())\n",
    "\n",
    "digit_sum = []\n",
    "while userInput > 0:\n",
    "    digit = userInput % 10\n",
    "    digit_sum.insert(0, str(digit))\n",
    "    userInput //= 10\n",
    "    \n",
    "print(\" \".join(digit_sum))"
   ]
  }
