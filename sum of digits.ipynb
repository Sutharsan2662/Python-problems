   "source": [
    "#sum of digits\n",
    "userInput = int(input())\n",
    "\n",
    "digit_sum = 0\n",
    "while userInput > 0:\n",
    "    digit = userInput % 10\n",
    "    digit_sum += digit\n",
    "    userInput //= 10\n",
    "    \n",
    "print(digit_sum)"
   ]
  }
