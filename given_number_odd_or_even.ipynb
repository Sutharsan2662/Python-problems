    "#check whether given number is odd or even\n",
    "UserInput = input()\n",
    "\n",
    "roundoff = round(float(UserInput))\n",
    "\n",
    "if roundoff%2 == 1:\n",
    "    print(\"Odd\")\n",
    "else:\n",
    "    print(\"Even\")"
